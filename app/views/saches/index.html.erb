<p id="notice"><%= notice %></p>

<h1>Saches</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Ma sach</th>
      <th>Ten sach</th>
      <th>Mtg</th>
      <th>Mtl</th>
      <th>Manxb</th>
      <th>Nxb</th>
      <th>Ten the loai</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saches.each do |sach| %>
      <tr>
        <td><%= sach.ma_sach %></td>
        <td><%= sach.ten_sach %></td>
        <td><%= sach.mtg %></td>
        <td><%= sach.mtl %></td>
        <td><%= sach.maNXB %></td>
        <td><%= sach.nxb %></td>
     
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sach', new_sach_path,class:"btn btn-primary" %>
